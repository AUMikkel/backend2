{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://localhost:27017/assign3",
          "collectionName": "logs",
          "cappedMaxSizeMb": "50",
          "cappedMaxDocuments": "1000",
          "DatabaseName": "assign3"
        }
      }
    ]
  },
  "MongoDBSettings": {
    "ConnectionString": "mongodb://localhost:27017/assign3",
    "DatabaseName": "assign3",
    "CollectionName": "logs"
  },
  "AllowedHosts": "*"
}
